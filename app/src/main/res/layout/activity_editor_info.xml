<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/activity_change_try"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/windowsBackground">

    <include
        android:id="@+id/myeditorchangename_head"
        layout="@layout/mind_circle_title_layout"/>

    <EditText
        android:id="@+id/edit_change"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/myeditorchangename_head"
        android:layout_marginLeft="@dimen/len_10"
        android:layout_marginRight="@dimen/len_10"
        android:layout_marginTop="@dimen/len_16"
        android:background="@color/white"
        android:gravity="top"
        android:textColor="@color/circle_text_color_second"
        android:textColorHint="@color/divider_color"
        android:inputType="text"
        android:paddingBottom="@dimen/len_12"
        android:paddingLeft="@dimen/len_20"
        android:paddingRight="@dimen/len_20"
        android:paddingTop="@dimen/len_12"
        android:textSize="@dimen/font_16"/>
</RelativeLayout>
